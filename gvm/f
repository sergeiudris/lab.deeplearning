#!/bin/bash


dock() {
    docker build -t sample.ml.gvm-samples .
    docker run  --rm \
                --name gvm-samples \
                --memory 16g \
                --cpus 4.000 \
                -it \
                -p 7788:7888 \
                -v "$(pwd)":/opt/app \
                 sample.ml.gvm-samples \
                 bash
}

"$@"